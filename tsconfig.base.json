{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "baseUrl": ".",
    "noEmit": true
  },
  "include": [
    "src/**/*.ts",
    "index.ts"
  ],
  "exclude": ["node_modules", "dist"] // Geliştiricinin önerisindeki exclude satırı
}
